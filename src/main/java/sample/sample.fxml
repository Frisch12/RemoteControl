<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" prefHeight="152.0" prefWidth="272.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="0.0" prefHeight="75.0" valignment="TOP" vgrow="ALWAYS" />
      <RowConstraints minHeight="0.0" prefHeight="25.0" valignment="BOTTOM" vgrow="NEVER" />
   </rowConstraints>
   <children>
      <ImageView fx:id="imageView" GridPane.rowIndex="0" />
      <Button onAction="#doSomething" mnemonicParsing="false" text="Button" GridPane.rowIndex="1" />
   </children>
</GridPane>
